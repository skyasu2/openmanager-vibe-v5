{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "duckduckgo-search": {
      "command": "npx",
      "args": ["-y", "duckduckgo-mcp-server"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "fetch-mcp": {
      "command": "npx",
      "args": ["-y", "@kazuph/mcp-fetch"]
    },
    "fetch-simple": {
      "command": "npx",
      "args": ["-y", "@odgrim/mcp-fetch"]
    },
    "openmanager-dev": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-fetch",
        "http://localhost:3000/api/mcp"
      ]
    },
    "openmanager-production": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-fetch",
        "https://openmanager-vibe-v5.vercel.app/api/mcp"
      ]
    }
  }
}
