{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "openmanager-vibe-v5",
  "headers": [
    {
      "source": "/api/mcp",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=300"
        },
        {
          "key": "X-Free-Tier-Mode",
          "value": "enabled"
        }
      ]
    },
    {
      "source": "/api/system/status",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=1800, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/api/health",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=60, stale-while-revalidate=300"
        }
      ]
    },
    {
      "source": "/api/cache/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=1800, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/api/ai/edge-v2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=300, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/version/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=600, stale-while-revalidate=1200"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/js/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(main|login)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        },
        {
          "key": "X-Timer-Stability",
          "value": "enabled"
        },
        {
          "key": "X-Environment-Detection",
          "value": "stable"
        },
        {
          "key": "X-Refresh-Prevention",
          "value": "active"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), payment=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/admin/((?!ai-agent).*)",
      "destination": "/admin/ai-agent",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "DISABLE_LOCAL_RAG": "true",
    "PREFER_SUPABASE_RAG": "true",
    "AI_ENGINE_PRIORITY": "supabase_rag,rule_based,mcp,google_ai",
    "VERCEL_HEALTH_OPTIMIZATION": "true",
    "HEALTH_CHECK_CACHE_HOURS": "6",
    "DISABLE_EXCESSIVE_HEALTH_CHECKS": "true",
    "DISABLE_CRON_JOBS": "true",
    "AI_STREAM_TIMEOUT_PREVENTION": "true",
    "NEXT_PUBLIC_FREE_TIER_MODE": "true",
    "VERCEL_HOBBY_PLAN": "true",
    "SERVERLESS_FUNCTION_TIMEOUT": "8",
    "MEMORY_LIMIT_MB": "40",
    "DISABLE_BACKGROUND_JOBS": "true",
    "ENABLE_QUOTA_PROTECTION": "true",
    "VERCEL_ANALYTICS_ENABLED": "true",
    "SPEED_INSIGHTS_ENABLED": "true",
    "CDN_CACHE_HIT_RATE_TARGET": "95",
    "API_RESPONSE_TIME_TARGET": "100",
    "VERCEL_TIMER_STABILITY": "true",
    "VERCEL_ENV_DETECTION_STABLE": "true",
    "VERCEL_AUTH_RETRY_DELAY": "10000",
    "VERCEL_INIT_DELAY": "1000",
    "VERCEL_SYNC_DEBOUNCE": "3000",
    "PREVENT_AUTO_REFRESH": "true",
    "STABLE_ENVIRONMENT_DETECTION": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "SKIP_ENV_VALIDATION": "false",
      "ESLINT_NO_DEV_ERRORS": "true",
      "BUILD_TIME_OPTIMIZATION": "true",
      "SKIP_SUPABASE_CONNECTION_CHECK": "true",
      "DISABLE_HEALTH_CHECK": "true",
      "HEALTH_CHECK_CONTEXT": "false",
      "VALIDATE_ENV_ON_BUILD": "true",
      "NEXT_DISABLE_DEVTOOLS": "1",
      "SKIP_E2E_TESTS": "true",
      "ENABLE_QA_TESTS": "false"
    }
  },
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "src/app/api/mcp/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/ai/query/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/ai/edge-v2/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/ai/google-ai/generate/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/servers/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/health/route.ts": {
      "maxDuration": 5
    },
    "src/app/api/cache/*/route.ts": {
      "maxDuration": 5
    },
    "src/app/api/version/*/route.ts": {
      "maxDuration": 5
    },
    "src/app/api/security/csp-report/route.ts": {
      "maxDuration": 5
    }
  }
}
