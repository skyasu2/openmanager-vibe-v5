{
  "version": 2,
  "name": "openmanager-vibe-v5",
  "alias": ["openmanager-vibe"],
  "regions": ["icn1"],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/ai/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/data-generator/**/*.ts": {
      "maxDuration": 45,
      "memory": 512
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, x-vercel-protection-bypass, x-vercel-set-bypass-cookie"
        }
      ]
    },
    {
      "source": "/api/health",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "ee2aGggamAVy7ti2iycFOXamwgjIhuhr"
        },
        {
          "key": "x-vercel-set-bypass-cookie",
          "value": "true"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "dashboard"
        }
      ],
      "destination": "/dashboard"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "true",
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "NEXT_TELEMETRY_DISABLED": "1",
      "CI": "true",
      "ESLINT_NO_DEV_ERRORS": "true",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  },
  "github": {
    "autoAlias": false,
    "silent": true,
    "enabled": true
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "_comment_crons_disabled": "과금 방지를 위해 모든 cron 작업 비활성화. 필요시 수동으로 /api/health 접근",
  "cleanUrls": true,
  "trailingSlash": false
}
