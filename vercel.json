{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 25,
      "memory": 512
    },
    "src/app/api/ai/**/*.ts": {
      "maxDuration": 45,
      "memory": 1024
    },
    "src/app/api/ai/logging/stream/route.ts": {
      "maxDuration": 35,
      "memory": 512
    },
    "src/app/api/data-generator/**/*.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "src/app/api/ai-agent/**/*.ts": {
      "maxDuration": 45,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600"
        }
      ]
    },
    {
      "source": "/api/ai/logging/stream",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "dashboard"
        }
      ],
      "destination": "/dashboard"
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "regions": ["icn1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "DISABLE_LOCAL_RAG": "true",
    "PREFER_SUPABASE_RAG": "true",
    "AI_ENGINE_PRIORITY": "supabase_rag,rule_based,mcp,google_ai",
    "VERCEL_HEALTH_OPTIMIZATION": "true",
    "HEALTH_CHECK_CACHE_HOURS": "6",
    "DISABLE_EXCESSIVE_HEALTH_CHECKS": "true",
    "DISABLE_CRON_JOBS": "true",
    "AI_STREAM_TIMEOUT_PREVENTION": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "DISABLE_LOCAL_RAG": "true",
      "VERCEL_HEALTH_OPTIMIZATION": "true",
      "DISABLE_CRON_JOBS": "true",
      "AI_STREAM_TIMEOUT_PREVENTION": "true"
    }
  }
}
