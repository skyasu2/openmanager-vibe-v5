{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/ai/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/data-generator/**/*.ts": {
      "maxDuration": 45,
      "memory": 512
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, x-vercel-protection-bypass, x-vercel-set-bypass-cookie"
        }
      ]
    },
    {
      "source": "/api/health",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "ee2aGggamAVy7ti2iycFOXamwgjIhuhr"
        },
        {
          "key": "x-vercel-set-bypass-cookie",
          "value": "true"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "dashboard"
        }
      ],
      "destination": "/dashboard"
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "regions": ["icn1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "crons": []
}
