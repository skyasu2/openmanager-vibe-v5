# ============================================================================
# Cloud Run AI Backend - Environment Variables
# Copy to .env and fill in your values
# ============================================================================

# AI API Keys
GROQ_API_KEY=your-groq-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key

# Supabase (for checkpointer and RAG)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_DIRECT_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Cloud Run API Secret (for authentication)
CLOUD_RUN_API_SECRET=your-api-secret
