{
  "name": "ai-engine",
  "version": "5.83.12",
  "description": "LangGraph Supervisor Engine for Cloud Run",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prompt:eval": "promptfoo eval -c promptfoo/promptfooconfig.yaml",
    "prompt:view": "promptfoo view",
    "prompt:redteam": "promptfoo eval -c promptfoo/redteam/security-tests.yaml",
    "prompt:cache:clear": "promptfoo cache clear"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.43",
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/langchain": "^1.0.113",
    "@hono/node-server": "^1.19.7",
    "@langchain/core": "^1.1.5",
    "@langchain/google-genai": "^2.1.0",
    "@langchain/groq": "^1.0.2",
    "@langchain/langgraph": "^1.0.4",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/langgraph-supervisor": "^1.0.0",
    "@langchain/mistralai": "^1.0.2",
    "@langfuse/core": "^4.5.1",
    "@langfuse/langchain": "^4.5.1",
    "@mistralai/mistralai": "^1.11.0",
    "@opentelemetry/api": "^1.9.0",
    "@supabase/supabase-js": "^2.47.7",
    "@upstash/redis": "^1.36.0",
    "ai": "^5.0.102",
    "dotenv": "^16.4.7",
    "hono": "^4.11.1",
    "js-tiktoken": "^1.0.21",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "promptfoo": "^0.120.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
