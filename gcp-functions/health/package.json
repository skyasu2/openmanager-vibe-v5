{
  "name": "health-check",
  "version": "2.0.0",
  "description": "Health Check for GCP Functions - 2025-11-20 업데이트",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=healthCheck",
    "deploy": "gcloud functions deploy health-check --runtime nodejs20 --trigger-http --allow-unauthenticated --memory=128MB --timeout=10s"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
