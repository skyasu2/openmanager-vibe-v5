{
  "mcpServers": {
    "openmanager-local": {
      "command": "node",
      "args": ["mcp-server/server.js"],
      "cwd": "D:/cursor/openmanager-vibe-v5",
      "env": {
        "NODE_ENV": "development",
        "MCP_ENV": "cursor"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "D:/cursor/openmanager-vibe-v5"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
    "git": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-git",
        "--repository",
        "D:/cursor/openmanager-vibe-v5"
      ]
    },
    "browser-tools": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-browser-tools"]
    }
  }
}
