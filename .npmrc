# Vercel 배포 최적화 설정
legacy-peer-deps=true

# 캐시 최적화  
prefer-offline=true

# 네트워크 안정성 개선 (429 Error 방지 강화)
fetch-retries=5
fetch-retry-mintimeout=15000
fetch-retry-maxtimeout=120000
fetch-timeout=300000

# Registry 안정성 개선
registry=https://registry.npmjs.org/
maxsockets=15
fetch-retry-factor=2

# 보안 설정 (429 에러 방지를 위해 moderate 유지)
audit-level=moderate

# GitHub Actions 최적화
progress=false
fund=false

# 로그 레벨 (터미널 노이즈 최소화)
loglevel=error

# Rate Limiting 방지 (timing 로그 비활성화)
timing=false