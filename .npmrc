# Vercel 배포 최적화 설정
legacy-peer-deps=true
auto-install-peers=true

# 캐시 최적화
prefer-offline=true
cache-max=86400000

# 네트워크 안정성 개선 (429 Error 방지 강화)
fetch-retries=5
fetch-retry-mintimeout=15000
fetch-retry-maxtimeout=120000
fetch-timeout=300000

# Registry 안정성 개선
registry=https://registry.npmjs.org/
maxsockets=15
request-retries=5

# 보안 설정 (429 에러 방지를 위해 moderate 유지)
audit-level=moderate

# GitHub Actions 최적화
progress=false
fund=false

# 로그 레벨
loglevel=warn

# Rate Limiting 방지
timing=true 