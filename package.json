    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "analyze": "cross-env ANALYZE=true npm run build",
    "deps:check": "npm list | grep -E \"(UNMET|missing:)\" || echo 'All dependencies satisfied'",
