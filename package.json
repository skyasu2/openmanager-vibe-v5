{
  "name": "openmanager-vibe-v5-clean",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules/.cache",
    "build:clean": "npm run clean && npm run build",
    "lint": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "npm run type-check && npm run lint",
    "test:enterprise": "node scripts/mockEnterpriseHit.ts",
    "enterprise:seed": "curl -X POST http://localhost:3001/api/enterprise/seed",
    "enterprise:simulate": "curl -X POST http://localhost:3001/api/simulate",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "health-check:prod": "curl -f https://openmanager-vibe-v5.vercel.app/api/health || exit 1",
    "perf-test": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "audit": "npm audit --audit-level=high",
    "build:analyze": "ANALYZE=true npm run build",
    "deploy:preview": "vercel --confirm",
    "deploy:prod": "vercel --prod --confirm",
    "monitor": "npm run health-check:prod && echo 'âœ… Production health check passed'"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.49.8",
    "@types/ioredis": "^4.28.10",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.12.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.23",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.50",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
