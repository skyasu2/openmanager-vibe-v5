    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "check": "biome check .",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "lint:strict": "biome lint --error-on-warnings .",
    "lint:progressive": "biome lint .",
    "lint:fast": "biome lint .",
    "lint:ci": "biome lint .",
    "lint:ci:strict": "biome lint --error-on-warnings .",
    "lint:quick": "biome lint .",
    "format": "biome format --write .",
    "format:check": "biome format .",
