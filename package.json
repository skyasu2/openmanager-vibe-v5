{
  "name": "openmanager-vibe-v5-clean",
  "version": "5.6.10",
  "description": "🤖 OpenManager AI - 지능형 AI 기반 서버 모니터링 시스템 (최적화된 코드베이스)",
  "private": true,
  "keywords": [
    "ai-agent",
    "server-monitoring",
    "ai-inference",
    "mcp",
    "nextjs",
    "typescript",
    "optimized"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules/.cache",
    "build:clean": "npm run clean && npm run build",
    "lint": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "npm run type-check && npm run lint",
    "test:enterprise": "node scripts/mockEnterpriseHit.ts",
    "enterprise:seed": "curl -X POST http://localhost:3001/api/enterprise/seed",
    "enterprise:simulate": "curl -X POST http://localhost:3001/api/simulate",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "health-check:prod": "curl -f https://openmanager-vibe-v5.vercel.app/api/health || exit 1",
    "perf-test": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "audit": "npm audit --audit-level=high",
    "build:analyze": "ANALYZE=true npm run build",
    "deploy:preview": "vercel --confirm",
    "deploy:prod": "vercel --prod --confirm",
    "monitor": "npm run health-check:prod && echo '✅ Production health check passed'",
    "init:ai-context": "node scripts/init-ai-context.js",
    "test:ai-engine": "node scripts/test-ai-engine.js",
    "test:python": "python --version && python -c \"import kats, pyod, scipy, sklearn; print('✅ All Python packages available')\"",
    "setup:python": "pip install -r src/modules/ai-agent/python-engine/requirements.txt",
    "setup:python-lightweight": "pip install -r src/modules/ai-agent/python-engine/requirements-lightweight.txt",
    "test:python-analysis": "node scripts/test-python-analysis.js",
    "test:optimized-ai": "node scripts/test-optimized-ai.js",
    "python:install": "python -m pip install --upgrade pip && npm run setup:python",
    "python:install-lightweight": "python -m pip install --upgrade pip && npm run setup:python-lightweight",
    "python:test-engine": "python src/modules/ai-agent/python-engine/engine_runner.py",
    "python:test-lightweight": "python src/modules/ai-agent/python-engine/lightweight_engine.py",
    "ai:optimize": "node scripts/test-optimized-ai.js",
    "ai:benchmark": "node scripts/test-optimized-ai.js --benchmark-only"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.49.8",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/ioredis": "^4.28.10",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.12.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.23",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.0-canary.51",
    "@types/node": "^20.17.50",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
