# =============================================================================
# üóÑÔ∏è Supabase Database (Required)
# =============================================================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_JWT_SECRET=your-jwt-secret-here
SUPABASE_DB_PASSWORD=your-db-password-here

# =============================================================================
# ü§ñ Google AI (Required for AI features)
# =============================================================================
GOOGLE_AI_ENABLED=true
GOOGLE_AI_API_KEY=your-google-ai-api-key-here
GOOGLE_AI_MODEL=gemini-2.0-flash
GOOGLE_AI_BETA_MODE=true
GOOGLE_AI_DAILY_LIMIT=1200
GOOGLE_AI_MINUTE_LIMIT=10
GOOGLE_AI_TPM_LIMIT=800000
GOOGLE_AI_QUOTA_PROTECTION=true

# =============================================================================
# üîê GitHub OAuth (Required for auth)
# =============================================================================
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret
NEXTAUTH_SECRET=generate-random-32-char-string
NEXTAUTH_URL=http://localhost:3000

# =============================================================================
# ‚òÅÔ∏è Cloud Run AI Engine (Optional)
# =============================================================================
CLOUD_RUN_AI_URL=https://ai-engine-xxx.asia-northeast1.run.app
CLOUD_RUN_API_SECRET=your-secure-api-secret-here

# =============================================================================
# üñ•Ô∏è GCP Settings (Optional)
# =============================================================================
VM_API_TOKEN=your-vm-api-token-here
GCP_PROJECT_ID=your-gcp-project-id

# =============================================================================
# üíæ Caching
# =============================================================================
MEMORY_CACHE_ENABLED=true
MEMORY_CACHE_MAX_SIZE=100
MEMORY_CACHE_TTL_SECONDS=900

# =============================================================================
# üìä Monitoring
# =============================================================================
MONITORING_ENABLED=true
METRICS_ENABLED=true
HEALTH_CHECK_ENABLED=true

# =============================================================================
# üîß Development
# =============================================================================
NODE_ENV=development
DEBUG_MODE=true
VERBOSE_LOGGING=false
