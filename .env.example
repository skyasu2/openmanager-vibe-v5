# OpenManager VIBE v5 환경변수 예제
# 실제 값으로 복사하여 .env.local로 저장하세요

# ====== 기본 설정 ======
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_FREE_TIER_MODE=true

# ====== Supabase 설정 ======
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ====== OpenAI 설정 ======
OPENAI_API_KEY=sk-your-openai-api-key-here

# ====== Google AI 설정 ======
GOOGLE_AI_API_KEY=AIza-your-google-ai-api-key-here

# ====== GitHub 설정 (선택사항) ======
GITHUB_TOKEN=ghp_your-github-token-here

# ====== 검색 API (선택사항) ======
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# ====== GCP 설정 (선택사항) ======
GCP_PROJECT_ID=your-project-id
GCP_REGION=asia-northeast3

# ====== 보안 설정 ======
NEXTAUTH_SECRET=your-random-secret-here
NEXTAUTH_URL=http://localhost:3000

# ====== 개발 모드 설정 ======
MOCK_MODE=dev
SKIP_ENV_VALIDATION=false
DISABLE_TELEMETRY=true

# ====== Vercel 최적화 ======
VERCEL_ANALYTICS_ENABLED=true
SPEED_INSIGHTS_ENABLED=true

# ==============================================
# 🔧 중앙집중식 설정 관리 (New System)
# ==============================================

# 🎯 서버 설정 (핵심)
TOTAL_SERVERS=15

# 🎭 Mock 시스템 설정
MOCK_ENABLED=true
MOCK_DATA_SOURCE=expanded
MOCK_AUTO_ROTATION=false
MOCK_UPDATE_INTERVAL=30000

# 🌐 API 설정
API_DEFAULT_PAGE_SIZE=10
API_MAX_PAGE_SIZE=50
API_ENABLE_PAGINATION=true
API_TIMEOUT_MS=10000

# ⚡ 성능 설정
ENABLE_CACHE=true
CACHE_TTL_MS=300000
BATCH_SIZE=100
MAX_CONCURRENT_REQUESTS=10

# 🌍 환경별 설정
ENABLE_DEBUG_LOGS=true
ENABLE_METRICS=true
ENABLE_HEALTH_CHECKS=true

# ==============================================
# 💡 시나리오별 권장 설정
# ==============================================

# 📊 개발 환경 (현재 설정)
# TOTAL_SERVERS=15, MOCK_DATA_SOURCE=expanded
# ENABLE_DEBUG_LOGS=true, ENABLE_CACHE=true

# 🔥 베르셀 무료 티어 최적화
# TOTAL_SERVERS=10, CACHE_TTL_MS=600000
# BATCH_SIZE=50, MAX_CONCURRENT_REQUESTS=5

# ⚡ 고성능 환경
# TOTAL_SERVERS=25, CACHE_TTL_MS=60000
# MAX_CONCURRENT_REQUESTS=20, BATCH_SIZE=200