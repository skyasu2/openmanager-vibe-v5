# OpenManager VIBE v5 환경변수 예제
# 실제 값으로 복사하여 .env.local로 저장하세요

# ====== 기본 설정 ======
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_FREE_TIER_MODE=true

# ====== Supabase 설정 ======
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ====== OpenAI 설정 ======
OPENAI_API_KEY=sk-your-openai-api-key-here

# ====== Google AI 설정 ======
GOOGLE_AI_API_KEY=AIza-your-google-ai-api-key-here

# ====== GitHub 설정 (선택사항) ======
GITHUB_TOKEN=ghp_your-github-token-here

# ====== 검색 API (선택사항) ======
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# ====== GCP 설정 (선택사항) ======
GCP_PROJECT_ID=your-project-id
GCP_REGION=asia-northeast3

# ====== 보안 설정 ======
NEXTAUTH_SECRET=your-random-secret-here
NEXTAUTH_URL=http://localhost:3000

# ====== 개발 모드 설정 ======
MOCK_MODE=dev
SKIP_ENV_VALIDATION=false
DISABLE_TELEMETRY=true

# 게스트 모드 설정 (개발/프로덕션 전환)
# full_access: 게스트가 모든 기능 사용 가능 (개발용)
# restricted: 게스트 접근 제한 (프로덕션용, 기본값)
NEXT_PUBLIC_GUEST_MODE=full_access

# ====== Vercel 최적화 ======
VERCEL_ANALYTICS_ENABLED=true
SPEED_INSIGHTS_ENABLED=true

# ====== Mock 시스템 설정 ======
MOCK_MODE=dev
MOCK_ENABLED=true
TOTAL_SERVERS=15

# ====== API 성능 설정 ======
API_TIMEOUT_MS=10000
CACHE_TTL_MS=300000
MAX_CONCURRENT_REQUESTS=10

# ====== 디버그 설정 ======
ENABLE_DEBUG_LOGS=false
ENABLE_METRICS=true

# ====== Vercel 배포 최적화 (프로덕션 전용) ======
# RAG 시스템 설정
DISABLE_LOCAL_RAG=true
PREFER_SUPABASE_RAG=true
AI_ENGINE_PRIORITY=supabase_rag,rule_based,mcp,google_ai

# 헬스 체크 최적화
VERCEL_HEALTH_OPTIMIZATION=true
HEALTH_CHECK_CACHE_HOURS=6
DISABLE_EXCESSIVE_HEALTH_CHECKS=true
DISABLE_CRON_JOBS=true

# AI 스트림 안정성
AI_STREAM_TIMEOUT_PREVENTION=true

# Vercel 플랫폼 설정
VERCEL_HOBBY_PLAN=true
SERVERLESS_FUNCTION_TIMEOUT=5
MEMORY_LIMIT_MB=64

# 최적화 전략
STATIC_JSON_OPTIMIZATION=true
DISABLE_BACKGROUND_JOBS=true
ENABLE_QUOTA_PROTECTION=true

# 성능 목표
CDN_CACHE_HIT_RATE_TARGET=95
API_RESPONSE_TIME_TARGET=100

# 타이머 안정성 (무한 새로고침 방지)
VERCEL_TIMER_STABILITY=true
VERCEL_ENV_DETECTION_STABLE=true
VERCEL_AUTH_RETRY_DELAY=10000
VERCEL_INIT_DELAY=1000
VERCEL_SYNC_DEBOUNCE=3000
PREVENT_AUTO_REFRESH=true
STABLE_ENVIRONMENT_DETECTION=true

# Edge 및 빌드 최적화
VERCEL_EDGE_MIDDLEWARE=true
VERCEL_BUILD_OPTIMIZATION=true
NEXT_OPTIMIZE_FONTS=true
NEXT_OPTIMIZE_IMAGES=true

# 디버그 모드
VERCEL_ANALYTICS_DEBUG=false
SPEED_INSIGHTS_DEBUG=false